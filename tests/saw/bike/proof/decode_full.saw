// Proof commands

// NOTE: This override is used for KEM functions
compute_syndrome_update_ov <- admit  "compute_syndrome"
    [ gf2x_mod_mul_ov4
    , gf2x_add_ov4
    , gf2x_add4_ov, gf2x_add3_ov
    , gf2x_add3_left_ov1, gf2x_add3_left_ov2, gf2x_add3_left_ov3
    , convert_to_redundant_rep_ov
    , transpose_ov
    , secure_clean_HACK_128_ov, secure_clean_100_ov64 ]
    compute_syndrome_update_spec;

decode_ov <- admit "decode"
    [ get_threshold_ov
    , compute_counter_of_unsat_ov
    , compute_syndrome_ov
    , fix_black_error_ov
    , fix_error1_ov
    , fix_gray_error_ov
    , count_ones_R_BITS_ov8
    , count_ones_R_BITS_ov
    , count_ones_N_SIZE_ov
    , count_ones_10_ov
    , cyclic_product_ov ]
    decode_spec;
