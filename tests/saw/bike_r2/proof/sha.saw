///////////////////////////////////////////////////////////////////////////////
// Specifications

let sha384_spec len = do {
  (_, mp) <- in_ref (i8_array_t len) "m";
  rp <- out_ref (i8_array_t SHA384_HASH_SIZE);
  crucible_execute_func [mp, tm {{ `len:[64] }}, rp];
  _ <- point_to (i8_array_t SHA384_HASH_SIZE) rp "r";
  crucible_return rp;
};


